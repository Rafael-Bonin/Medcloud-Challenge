// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Patient {
  id Int @id @default(autoincrement())
  name String
  birth_date DateTime
  email String @unique
  address String

  @@map("patients")
}

model Employee {
  id Int @id @default(autoincrement())
  name String
  email String @unique
  role String
  password String

  @@map("employees")
}